{
    "env":{
        "browser":true
    },
    "extends":"eslint:recommended",
    "parserOptions":{
        "sourceType":"module"
    },
    "rules":{
        "no-console":0,

        "quotes":[
            "error",
            "double"
        ],
        "semi":[
            "error",
            "always"
        ]
    }
}
